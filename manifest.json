{
  "manifest_version": 2,
  "name": "Audrey's Toolkit",
  "description": "Audrey's Toolkit",
  "version": "0.1.1",
  "permissions": ["<all_urls>"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "Audrey's Toolkit"
  }
}